  <inclBatchRun>true</inclBatchRun>
  <inclRandomSeed>true</inclRandomSeed>
  <scheduleParams>
    <start>1.0</start>
    <interval>2.0</interval>
    <priority>-Infinity</priority>
    <duration>-1.0</duration>
    <frequency>REPEAT</frequency>
  </scheduleParams>
  <methodDataSources class="linked-hash-map">
    <entry>
      <string>Average D conjugations</string>
      <repast.simphony.data2.engine.MethodDataSourceDefinition>
        <id>Average D conjugations</id>
        <className>org.holistic.bactocom.Bacterium</className>
        <methodName>getDonorConjugations</methodName>
        <aggType>MEAN</aggType>
      </repast.simphony.data2.engine.MethodDataSourceDefinition>
    </entry>
    <entry>
      <string>Average T conjugations</string>
      <repast.simphony.data2.engine.MethodDataSourceDefinition>
        <id>Average T conjugations</id>
        <className>org.holistic.bactocom.Bacterium</className>
        <methodName>getTransconjugantConjugations</methodName>
        <aggType>MEAN</aggType>
      </repast.simphony.data2.engine.MethodDataSourceDefinition>
    </entry>
  </methodDataSources>
  <countSources class="linked-hash-map"/>
  <customNADataSources class="linked-hash-map"/>
  <customAggDataSources class="linked-hash-map"/>
</repast.simphony.data2.engine.DataSetDescriptor>